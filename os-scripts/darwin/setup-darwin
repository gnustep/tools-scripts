#!/bin/sh
export DYLD_LIBRARY_PATH=/usr/local/Cellar/gcc/10.2.0/lib/gcc/10:${DYLD_LIBRARY_PATH}
export LD_LIBRARY_PATH=/usr/local/Cellar/gcc/10.2.0/lib/gcc/10:${LD_LIBRARY_PATH}
echo ${DYLD_LIBRARY_PATH}
#!/usr/bin/env bash
set -euo pipefail
REPO_ROOT="$(cd "$(dirname "$0")/../.." && pwd)"
if [ -f "$REPO_ROOT/setup-darwin" ]; then
  exec bash "$REPO_ROOT/setup-darwin" "$@"
else
  echo "setup-darwin not found in repo root" >&2
  exit 1
fi
